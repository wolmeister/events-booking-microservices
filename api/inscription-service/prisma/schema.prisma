generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Inscription {
  id              String    @id @default(uuid())
  userId          String
  eventId         String
  certificateCode String?
  checkintAt      DateTime?
  createdAt       DateTime  @default(now())
}
